{
    "name": "noonenew/laramapleaflet",
    "description": "Simple package to facilitate and automate the use of maps in Laravel 5.x using Dhmtlx Gantt v7 library",
    "keywords": [
      "leaflet","laravel5","gantt","noonenew"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "M",
            "email": "mplanmaster@gmail.com"
        }
    ],
    "require": {
      "php": ">=5.6.4",
      "illuminate/support": "~5.1"
    },

    "autoload": {
      "psr-4": {
        "Noonenew\\LaravelGanttChart\\" : "src/"
      }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Noonenew\\LaravelGanttChart\\Providers\\LeafletServiceProvider"
            ]
        }
    }
}
